                        <TableColumn fx:id="eventoNomeTableColumn" prefWidth="255.0" text="Evento" />
                        <TableColumn fx:id="giornoTableColumn" prefWidth="266.0" text="Giorno" />
                        <TableColumn fx:id="fasciaTableColumn" prefWidth="211.0" text="Fascia" />
