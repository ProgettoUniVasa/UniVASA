            <Label fx:id="farmaciLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="76.0" layoutY="13.0" prefHeight="39.0" prefWidth="612.0" text="I tuoi Turni di Lavoro" textAlignment="CENTER" textFill="ORANGE">
               <font>
                  <Font name="Palatino Linotype Bold" size="30.0" />
               </font>
            </Label>
            <VBox layoutY="51.0" prefHeight="517.0" prefWidth="763.0">
               <children>
                  <TableView fx:id="lavoroTable" prefHeight="487.0" prefWidth="632.0" style="-fx-background-color: orange;">
