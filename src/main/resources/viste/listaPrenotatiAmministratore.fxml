<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="507.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.ing.univasa.controller.amministratorecontroller.ListaPrenotatiAmministratoreController">
   <children>
      <Label fx:id="prenotatiLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="39.0" prefWidth="766.0" text="Lista Prenotati" textAlignment="CENTER" textFill="ORANGE">
         <font>
            <Font name="Palatino Linotype Bold" size="30.0" />
         </font>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="66.0" prefWidth="690.0">
         <children>
            <Button fx:id="indietroButton" mnemonicParsing="false" onAction="#indietroAction" prefHeight="39.0" prefWidth="92.0" text="Indietro" />
         </children>
      </HBox>
      <TableView fx:id="prenotatiTable" prefHeight="370.0" prefWidth="678.0" style="-fx-background-color: orange;">
        <columns>
          <TableColumn fx:id="nomeTableColumn" prefWidth="117.0" text="Nome" />
            <TableColumn fx:id="cognomeTableColumn" prefWidth="134.0" text="Cognome" />
            <TableColumn fx:id="emailTableColumn" prefWidth="186.0" text="Email" />
            <TableColumn fx:id="telefonoTableColumn" prefWidth="135.0" text="Telefono" />
            <TableColumn fx:id="matricolaTableColumn" prefWidth="117.0" text=" Matricola" />
        </columns>
         <VBox.margin>
            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
         </VBox.margin>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" />
   </padding>
</VBox>
