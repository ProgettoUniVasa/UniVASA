<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.ing.univasa.controller.operatorecontroller.EventoOperatoreController">
   <children>
      <AnchorPane>
         <children>
            <Label fx:id="farmaciLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="8.0" layoutY="13.0" prefHeight="39.0" prefWidth="612.0" text="                  Iscritti all'evento" textAlignment="CENTER" textFill="ORANGE">
               <font>
                  <Font name="Palatino Linotype Bold" size="30.0" />
               </font>
            </Label>
            <VBox layoutY="51.0" prefHeight="517.0" prefWidth="763.0">
               <children>
                  <TableView fx:id="eventoTable" prefHeight="487.0" prefWidth="632.0" style="-fx-background-color: orange;">
                     <columns>
                        <TableColumn fx:id="nomeTableColumn" prefWidth="159.0" text="Nome" />
                        <TableColumn fx:id="cognomeTableColumn" prefWidth="178.0" text="Cognome" />
                        <TableColumn fx:id="emailTableColumn" prefWidth="169.0" text="Email" />
                        <TableColumn fx:id="matricolaTableColumn" prefWidth="111.0" text="Matricola" />
                        <TableColumn fx:id="segnaVotazioneTableColumn" prefWidth="115.0" text="Segna votazione" />
                     </columns>
                     <VBox.margin>
                        <Insets bottom="10.0" left="5.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </TableView>
               </children>
               <padding>
                  <Insets bottom="10.0" left="5.0" right="10.0" />
               </padding>
            </VBox>
            <Button layoutX="690.0" layoutY="13.0" mnemonicParsing="false" onAction="#indietroAction" text="Indietro" />
         </children>
      </AnchorPane>
   </children>
</VBox>
