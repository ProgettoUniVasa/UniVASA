<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.layout.AnchorPane?>
<VBox prefHeight="517.0" prefWidth="763.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.ing.univasa.controller.operatorecontroller.CaricaVotiOperatoreController">
   <padding>
      <Insets bottom="10.0" left="5.0" right="10.0" />
   </padding>
      <AnchorPane>
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutY="14.0" prefHeight="39.0" prefWidth="829.0" text="Lista Candidati" textAlignment="CENTER" textFill="ORANGE">
               <font>
                  <Font name="Palatino Linotype Bold" size="30.0" />
               </font>
            </Label>
            <VBox layoutX="10.0" layoutY="61.0" prefHeight="517.0" prefWidth="763.0">
               <padding>
                  <Insets bottom="10.0" left="5.0" right="10.0" />
               </padding>
               <children>
                  <TableView fx:id="votiTable" prefHeight="455.0" prefWidth="748.0" style="-fx-background-color: orange;">
                     <columns>
                        <TableColumn fx:id="nomeTableColumn" prefWidth="199.0" text="Nome Candidato" />
                        <TableColumn fx:id="cognomeTableColumn" prefWidth="198.0" text="Cognome Candidato" />
                        <TableColumn fx:id="emailTableColumn" minWidth="0.0" prefWidth="199.0" text="Email Candidato" />
                        <TableColumn fx:id="azioneTableColumn" prefWidth="151.0" text="Azione" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
            <Button layoutX="711.0" layoutY="14.0" mnemonicParsing="false" onAction="#indietroAction" text="Indietro" />
         </children>
      </AnchorPane>
</VBox>
