<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.ing.univasa.controller.operatorecontroller.CaricaVotiOperatoreController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutY="14.0" prefHeight="39.0" prefWidth="829.0" text="Lista Eventi" textAlignment="CENTER" textFill="#0abab5">
         <font>
            <Font name="Palatino Linotype Bold" size="30.0" />
         </font>
      </Label>
      <VBox layoutX="10.0" layoutY="61.0" prefHeight="517.0" prefWidth="763.0">
         <padding>
            <Insets bottom="10.0" left="5.0" right="10.0" />
         </padding>
         <children>
            <TableView fx:id="votiTable" prefHeight="455.0" prefWidth="748.0" style="-fx-background-color: #0ABAB5;">
               <columns>
                  <TableColumn fx:id="nomeTableColumn" prefWidth="194.0" text="Nome Candidato" />
                  <TableColumn fx:id="cognomeTableColumn" prefWidth="154.0" text="Cognome Candidato" />
                  <TableColumn fx:id="emailTableColumn" minWidth="0.0" prefWidth="164.0" text="Email Candidato" />
                  <TableColumn fx:id="azioneTableColumn" prefWidth="126.0" text="Azione" />
               </columns>
            </TableView>
         </children>
      </VBox>
      <Button layoutX="711.0" layoutY="14.0" mnemonicParsing="false" onAction="#indietroAction" text="Indietro" />
