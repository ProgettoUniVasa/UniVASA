<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="594.0" prefWidth="1051.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.ing.univasa.controller.amministratorecontroller.ListaReportAmministratoreController">
   <children>
      <Label fx:id="reportLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="39.0" prefWidth="1185.0" text="Lista Report" textAlignment="CENTER" textFill="ORANGE">
         <font>
            <Font name="Palatino Linotype Bold" size="30.0" />
         </font>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="61.0" prefWidth="1031.0">
         <children>
            <Button fx:id="indietroButton" mnemonicParsing="false" onAction="#indietroAction" prefHeight="42.0" prefWidth="85.0" text="Indietro" />
         </children>
      </HBox>
      <TableView fx:id="reportTable" prefHeight="464.0" prefWidth="743.0" style="-fx-background-color: orange;">
        <columns>
          <TableColumn fx:id="nomeTableColumn" prefWidth="106.0" text="Nome Evento" />
            <TableColumn fx:id="regolamentoTableColumn" prefWidth="211.0" text="Regolamento" />
            <TableColumn fx:id="dataInizioTableColumn" prefWidth="71.0" text="Data Inizio" />
            <TableColumn fx:id="dataFineTableColumn" prefWidth="65.0" text="Data Fine" />
            <TableColumn fx:id="luogoTableColumn" prefWidth="107.0" text="Luogo" />
            <TableColumn fx:id="report_risultatiTableColumn" prefWidth="128.0" text="Report Risultati" />
            <TableColumn fx:id="report_statisticheTableColumn" prefWidth="131.0" text="Report Statistiche" />
            <TableColumn fx:id="modificaTableColumn" prefWidth="136.0" text="Aggiungi Report" />
           </columns>
         <VBox.margin>
            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
         </VBox.margin>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" />
   </padding>
</VBox>
