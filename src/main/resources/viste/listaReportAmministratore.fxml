<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="563.0" prefWidth="748.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.ing.univasa.controller.amministratorecontroller.ListaReportAmministratoreController">
   <children>
      <Label fx:id="reportLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="39.0" prefWidth="1185.0" text="Lista Report" textAlignment="CENTER" textFill="#0abab5">
         <font>
            <Font name="Palatino Linotype Bold" size="30.0" />
         </font>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="aggiungiReportButton" mnemonicParsing="false" onAction="#aggiungiReportAction" prefHeight="37.0" prefWidth="156.0" text="Aggiungi Report">
         <VBox.margin>
            <Insets bottom="10.0" left="580.0" right="5.0" />
         </VBox.margin>
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <TableView fx:id="reportTable" prefHeight="464.0" prefWidth="743.0" style="-fx-background-color: #0ABAB5;">
        <columns>
          <TableColumn fx:id="nomeTableColumn" prefWidth="87.0" text="Nome Evento" />
            <TableColumn fx:id="dataOraInizioTableColumn" prefWidth="96.0" text="Data Ora Inizio" />
            <TableColumn fx:id="dataOraFineTableColumn" prefWidth="97.0" text="Data Ora Fine" />
            <TableColumn fx:id="luogoTableColumn" prefWidth="84.0" text="Luogo" />
            <TableColumn fx:id="report_risultatiTableColumn" prefWidth="119.0" text="Report Risultati" />
            <TableColumn fx:id="report_statisticheTableColumn" prefWidth="143.0" text="Report Statistiche" />
            <TableColumn fx:id="eliminaTableColumn" minWidth="0.0" prefWidth="91.0" text="Elimina" />
        </columns>
         <VBox.margin>
            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
         </VBox.margin>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" />
   </padding>
</VBox>
