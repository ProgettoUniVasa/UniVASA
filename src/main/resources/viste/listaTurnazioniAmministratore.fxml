<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="568.0" prefWidth="761.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.ing.univasa.controller.amministratorecontroller.ListaTurnazioniAmministratoreController">
   <children>
      <Label fx:id="turniLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="39.0" prefWidth="766.0" text="Lista Turni" textAlignment="CENTER" textFill="#0abab5">
         <font>
            <Font name="Palatino Linotype Bold" size="30.0" />
         </font>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#indietroAction" text="Indietro" />
            <Button fx:id="aggiungiTurnoButton" mnemonicParsing="false" onAction="#aggiungiTurnoAction" onMouseClicked="#aggiungiTurnoAction" prefHeight="37.0" prefWidth="156.0" text="Aggiungi Turno">
               <font>
                  <Font size="16.0" />
               </font>
               <HBox.margin>
                  <Insets left="520.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <TableView fx:id="turnoTable" prefHeight="463.0" prefWidth="599.0" style="-fx-background-color: #0ABAB5;">
        <columns>
          <TableColumn fx:id="emailTableColumn" prefWidth="201.0" text="Email operatore" />
            <TableColumn fx:id="fasciaTableColumn" prefWidth="99.0" text="Fascia Oraria" />
            <TableColumn fx:id="data_turnoTableColumn" prefWidth="108.0" text="Data Turno" />
            <TableColumn fx:id="nome_eventoTableColumn" prefWidth="119.0" text="Nome Evento" />
            <TableColumn fx:id="luogoTableColumn" prefWidth="112.0" text="Luogo" />
            <TableColumn fx:id="eliminaTableColumn" minWidth="0.0" prefWidth="91.0" text="Elimina" />
        </columns>
         <VBox.margin>
            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
         </VBox.margin>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" />
   </padding>
</VBox>
